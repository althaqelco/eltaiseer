{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/new-damietta/**",
        "destination": "/new-damietta/index.html"
      },
      {
        "source": "/new-mansoura/**",
        "destination": "/new-mansoura/index.html"
      },
      {
        "source": "/property/**",
        "destination": "/properties/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
